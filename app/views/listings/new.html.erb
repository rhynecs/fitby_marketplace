<h2>New Listing</h2>

<%= form_with object: @listing, scope: :listing do |f| %>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :body %>
    <%= f.text_area :body, size:"70x5" %>
  </p>
  <p>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </p>
  <p>
    <%= f.label :price %>
    <%= f.text_field :price %>
  </p>
  <p>
  <%= f.submit "Sign up" %>
  </p>
<% end %>
