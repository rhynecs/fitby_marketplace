<h2>New Listing</h2>

<%= form_with model: [@user, @user.listings.build] do |f| %>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :body %>
    <%= f.text_area :body, size:"70x5" %>
  </p>
  <p>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </p>
  <p>
    <%= f.label :price %>
    <%= f.text_field :last_name %>
  </p>
  <p>
  <%= f.submit "Sign up" %>
  </p>
<% end %>

<%= render "devise/shared/links" %>
